   "cell_type": "code",
   "execution_count": 30,
   "id": "legislative-substance",
   "metadata": {},
   "outputs": [],
   "source": [
    "def Matsuno(u0,u1, cfl, ntimes=100, npts=91):\n",
    "    u_new = np.zeros(npts)\n",
    "    u_t0 = u0.copy()\n",
    "    u_t1 = u1.copy()\n",
    "    for k in range(ntimes):\n",
    "        u_t1[1:npts] = u_t0[1:npts] - cfl*(u_t0[1:npts] - u_t0[0:npts-1])#Predict the halfway solution between u^n and u^(n+1)\n",
    "        u_new[1:npts] = u_t0[1:npts] - cfl*u_t1[1:npts]\n",
    "    u_new[0] = u_new[-1]\n",
    "    u_t1=u_new.copy()\n",
    "    u_t0=u_t1.copy()\n",
    "    return u_new\n",
    "        #How can we apply a corrector to this solution?"
